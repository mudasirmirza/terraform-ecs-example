[
  {
    "cpu": 1024,
    "essential": true,
    "image": "${image_url}",
    "memory": 512,
    "name": "${container_name}",
    "portMappings": [
      {
        "containerPort": ${container_port},
        "hostPort": ${host_port}
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group_name}",
        "awslogs-region": "${log_group_region}"
      }
    }
  }
]
